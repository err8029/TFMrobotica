function send_velocity(lin,ang,robot_num)
    global vel_pub
    global vel2_pub

    if robot_num==1
        pub=vel_pub;
    else
        pub=vel2_pub;
    end

    msg_vel = rosmessage(vel_pub);
    msg_vel.Linear.X=lin;
    msg_vel.Linear.Y=lin;
    msg_vel.Linear.Z=lin;
    msg_vel.Angular.X=ang;
    send(pub,msg_vel)
end

