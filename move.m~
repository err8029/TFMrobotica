function move(obj,event)
    %Obtain the tag for the pressed button
    ID=obj.Tag;
    global cam1_msg
    global cam2_msg
    global img2_handle
    global img1_handle
    %goba vars for camera
    %check which button has been pressed
    if strcmp('up',ID)
        for c=1:1:100
            send_velocity(1,0)
        end
    end
    if strcmp('down',ID)
        for c=1:1:100
            send_velocity(-1,0)
        end
        display_image(cam1_msg,img1_handle)
    end
    %move left robot 1
    if strcmp('left',ID)
        for c=1:1:100
            send_velocity(0,1)
        end
        display_image(cam1_msg,img1_handle)
    end
    %move right robot 1
    if strcmp('right',ID)
        for c=1:1:100
            send_velocity(0,-1)
        end
        cam1_msg=img2_sub.LatestMessage;
        display_image(cam1_msg,img1_handle)
    end
    if strcmp('up2',ID)
        for c=1:1:100
            send_velocity2(1,0)
        end
        cam2_msg=img_sub.LatestMessage;
        display_image(cam2_msg,img2_handle)
    end
    if strcmp('down2',ID)
        for c=1:1:100
            send_velocity2(-1,0)
        end
        cam2_msg=img_sub.LatestMessage;
        display_image(cam2_msg,img2_handle)
    end
    if strcmp('left2',ID)
        for c=1:1:100
            send_velocity2(0,1)
        end
        cam2_msg=img_sub.LatestMessage;
        display_image(cam2_msg,img2_handle)
    end
    if strcmp('right2',ID)
        for c=1:1:100
            send_velocity2(0,-1)
        end
        cam2_msg=img_sub.LatestMessage;
        display_image(cam2_msg,img2_handle)
    end
end